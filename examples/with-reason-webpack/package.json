  },
  "devDependencies": {
    "bs-platform": "^8.0.3",
    "concurrently": "^4.0.1",
    "css-loader": "^3.5.3",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.11.2",
    "less-loader": "^6.1.0",
    "style-loader": "^1.2.1",
