{
  "private": true,
  "name": "with-preact",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "jest": "^29.0.3",
    "jest-preset-preact": "^1.0.0",
    "preact-cli": "^3.0.0",
    "sirv-cli": "1.0.3"
  },
  "dependencies": {
    "preact": "^10.11.2",
    "preact-render-to-string": "^5.2.6",
    "preact-router": "^3.2.1",
    "rsuite": "^5.21.0"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
