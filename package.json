{
  "name": "rsuite",
  "version": "4.2.1",
  "description": "A suite of react components",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "es/index.js",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "format": "prettier --write \"{src,test}/**/*.{tsx,ts,js}\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{tsx,ts,js}\"",
    "dist-style": "gulp buildStyle",
    "dist-dev": "cross-env NODE_ENV=development  webpack --mode development --progress",
    "dist-pro": "webpack --mode production --progress",
    "dist": "npm run dist-dev && npm run dist-pro",
    "build-gulp": "gulp build",
    "build": "npm run build-gulp && npm run dist",
    "dev": "cross-env NODE_ENV=development gulp dev",
    "lint-style": "stylelint  \"styles/**/*.less\"",
    "lint-ts": "eslint src/**/*.{ts,tsx}",
    "lint-js": "eslint src/**/*.js",
    "lint": "npm run lint-ts && npm run lint-style",
    "tdd": "karma start",
    "test": "npm run format:check && npm run lint && cross-env NODE_ENV=coverage karma start --single-run && npm run test-styles",
    "test-styles": "node test/stylesSpec.js",
    "coveralls": "cat ./coverage/lcov/lcov.info | ./node_modules/.bin/coveralls",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "rsuite",
    "component",
    "react-component"
  ],
  "author": "HYPERS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:rsuite/rsuite.git"
  },
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist",
    "lib",
    "src",
    "es",
    "styles"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "classnames": ">=2.0.0",
    "date-fns": "^1.30.1",
    "dom-lib": "^1.2.1",
    "element-resize-event": "^3.0.3",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react-lifecycles-compat": "^3.0.4",
    "react-virtualized": "^9.21.0",
    "recompose": "^0.26.0",
    "rsuite-table": "^3.8.4",
    "rsuite-utils": "^2.2.1",
    "schema-typed": "^1.4.0"
  },
  "peerDependencies": {
    "react": ">=16.6.0",
    "react-dom": ">=16.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-proto-to-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@types/classnames": "^2.2.8",
    "@types/lodash": "^4.14.134",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/recompose": "^0.30.6",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "autoprefixer": "^8.3.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-date-fns": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-dev": "^2.0.1",
    "brfs": "^1.6.1",
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "cross-env": "^6.0.3",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "dtslint": "^0.3.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "glob": "^7.1.4",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-less": "^4.0.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-rtlcss": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-es5-shim": "^0.0.4",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0-rc.6",
    "less-loader": "^4.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "make-dir": "^1.3.0",
    "mocha": "^2.5.3",
    "prettier": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.12.0",
    "style-loader": "^0.20.3",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "tinycolor2": "^1.4.1",
    "typescript": "^3.7.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 10"
  ],
  "sideEffects": [
    "lib/**/styles/*",
    "es/**/styles/*",
    "dist/*",
    "*.less",
    "*.css"
  ]
}
